@article{SilvapulleScore1995,
	jstor_articletype = {research-article},
	title = {A Score Test Against One-Sided Alternatives},
	author = {Silvapulle, Mervyn J. and Silvapulle, Paramsothy},
	journal = {Journal of the American Statistical Association},
	jstor_issuetitle = {},
	volume = {90},
	number = {429},
	jstor_formatteddate = {Mar., 1995},
	pages = {pp. 342-349},
	url = {http://www.jstor.org/stable/2291159},
	ISSN = {01621459},
	abstract = {A score-type statistic, T<sub>s</sub>, is introduced for testing H: ψ = 0 against K: ψ ≥ 0 and more general one-sided hypotheses when nuisance parameters may be present; ψ is a vector parameter. The main advantages of T<sub>s</sub> are that it requires estimation of the model only under the null hypothesis and that, it is asymptotically equivalent to the likelihood ratio statistic; these are precisely the reasons for the popularity of the score tests for testing against two-sided alternatives. In this sense, T<sub>s</sub> preserves the main attractive features of the classical two-sided score test. The theoretical results are presented in a general framework where the likelihood-based score function is replaced by an estimating function so that the test is applicable even if the exact population distribution is unknown. Computation of T<sub>s</sub> is simplified by the fact that it can be computed easily once the corresponding two-sided statistic has been computed. The relevance and simplicity of T<sub>s</sub> are illustrated by discussing a data example in detail. This example involves an autoregressive conditional heteroscedasticity (ARCH) model, and the objective is to test for the presence of ARCH effect. The null and alternative hypotheses turn out to be of the form H: ψ = 0 and K: ψ ≥ 0 respectively where ψ is the vector of ARCH parameters. In contrast to the likelihood ratio and other equivalent forms that are currently available for testing against such one-sided hypotheses, we note the following: T<sub>s</sub> is convenient to apply, because the full ARCH model need not be estimated subject to the inequality constraint ψ ≥ 0 and we do not need to know the exact likelihood because T<sub>s</sub> is based on estimating equations rather than likelihoods.},
	language = {English},
	year = {1995},
	publisher = {American Statistical Association},
	copyright = {Copyright © 1995 American Statistical Association},
}


@article{Silvapulle:1995:HTT:219373.219391,
	author = {Silvapulle, Mervyn J.},
	title = {A {H}otelling's {$T^2$}-type statistic for testing against one-sided hypotheses},
	journal = {J. Multivar. Anal.},
	issue_date = {Nov. 1995},
	volume = {55},
	number = {2},
	month = nov,
	year = {1995},
	issn = {0047-259X},
	pages = {312--319},
	numpages = {8},
	url = {http://dx.doi.org/10.1006/jmva.1995.1081},
	doi = {10.1006/jmva.1995.1081},
	acmid = {219391},
	publisher = {Academic Press, Inc.},
	address = {Orlando, FL, USA},
	sep_note = "specifically looking at the Hotelling T2 for testing zero mean versus alternative of mean in the positive orthant. Shown to be asymptotically equivalent to Perlman's LRT, but to have different power in finite samples. Illustrates some differences graphically. As in Perlman's test, the weights of the chi-bar square which determine the survival function of the one-sided Hotelling statistic depend on the unknown covariance parameter. Also as in Perlman, the maximal value of the weights over all possible covariance matrices is taken to get a p-value, so the test can be very conservative. This has consequences for use in finance, since it implies some disconnect between the sample statistic and the underlying population parameters (and that disconnect changes character as the sample size increases). silvapulle_hotelling.pdf"
}


