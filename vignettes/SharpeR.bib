@article {Willink1823,
	title = {A confidence interval and test for the mean of an asymmetric distribution},
	journal = {Communications in Statistics - Theory and Methods},
	volume = {34},
	year = {2005},
	pages = {753-766},
	abstract = {A confidence interval and test are obtained for the mean of an asymmetric distribution using a random sample of size n. The method is based on N. J. Johnson{\textquoteright}s (1978) modified t-test, where terms of Cornish-Fisher expansions involving the third moment are used to adjust the conventional statistic to have more closely a Student{\textquoteright}s t-distribution with n - 1 degrees of freedom. Johnson{\textquoteright}s (1978) test cannot be inverted uniquely, so a corresponding confidence interval for the mean may be disjointed. However, an artificial term of small order can be added to make inversion of the test a uniquely defined operation, which prevents such disjointedness. The resulting one-sided and two-sided intervals perform better than others in the literature with skewed distributions, and have good performance with a normal distribution. The two-sided interval may be recommended for general use if the sample size is 10 or more and the nominal confidence coefficient is 95\% or less, or if the sample size is 30 or more and the confidence coefficient is 99\% or less. Copyright {\textcopyright} Taylor \& Francis, Inc.},
	keywords = {Approximation theory, Cornish-Fisher expansion, Degrees of freedom (mechanics), Distribution-free, Estimation, Monte Carlo methods, Nonnormal, Probability distributions, Random processes, Skewness, Students, T-distribution, T-test},
	doi = {10.1081/STA-200054419},
